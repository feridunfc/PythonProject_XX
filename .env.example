# En az birini doldurun; yoksa FORCE_PROVIDER=mock kullanın
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Davranış
FORCE_PROVIDER=openai   # openai | anthropic | mock
AI_ALLOW_MOCK=1
LOG_LEVEL=INFO
# Observability
OBS_BACKENDS=jsonl            # jsonl[,otel]
TRACE_SERVICE_NAME=pythonproject_xx
TRACE_JSONL_PATH=trace_log.jsonl

# OpenTelemetry (opsiyonel; collector varsa doldurun)
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318
