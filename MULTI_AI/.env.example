# ---- Provider keys ----
# En az birini doldurun
GEMINI_API_KEY=
# GOOGLE_API_KEY=        # alternatif isim
# OPENAI_API_KEY=
# DEEPSEEK_API_KEY=

# ---- Provider seçimi / davranış ----
